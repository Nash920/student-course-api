{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
